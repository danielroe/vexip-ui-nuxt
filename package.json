{
  "name": "@vexip-ui/nuxt",
  "version": "0.0.0",
  "type": "module",
  "license": "MIT",
  "author": "qmhc",
  "description": "Nuxt module for Vexip UI",
  "packageManager": "pnpm@8.2.0",
  "scripts": {
    "changelog": "changelogen --release",
    "dev": "nuxi dev playground",
    "dev:build": "nuxi build playground",
    "dev:prepare": "nuxt-module-build --stub && nuxi prepare playground",
    "lint": "eslint .",
    "postpublish": "pinst --enable",
    "precommit": "lint-staged -c ./.husky/.lintstagedrc -q",
    "preinstall": "npx only-allow pnpm",
    "prepack": "nuxt-module-build",
    "prepare": "is-ci || husky install",
    "prepublishOnly": "pinst --disable",
    "publish:ci": "tsx scripts/publish.ts",
    "release": "tsx scripts/release.ts",
    "test": "vitest run",
    "test:watch": "vitest watch"
  },
  "main": "dist/module.cjs",
  "module": "dist/module.mjs",
  "types": "dist/types.d.ts",
  "exports": {
    ".": {
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs",
      "types": "./dist/types.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "homepage": "https://github.com/vexip-ui/vexip-ui/tree/main/common/nuxt#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vexip-ui/vexip-ui.git",
    "directory": "common/nuxt"
  },
  "dependencies": {
    "@nuxt/kit": "^3.4.3",
    "@rollup/pluginutils": "^5.0.2",
    "@vexip-ui/utils": "^1.11.1",
    "compare-versions": "6.0.0-rc.1",
    "local-pkg": "^0.4.2",
    "magic-string": "^0.30.0",
    "unplugin": "^1.3.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.6.3",
    "@nuxt/module-builder": "^0.3.0",
    "@nuxt/schema": "^3.4.3",
    "@nuxt/test-utils": "^3.4.3",
    "@types/minimist": "^1.2.2",
    "@types/node": "^18",
    "@types/prompts": "^2.4.4",
    "@types/semver": "^7.5.0",
    "@vexip-ui/commitlint-config": "^0.1.0",
    "@vexip-ui/eslint-config": "^0.5.2",
    "@vexip-ui/icons": "^1.2.0",
    "changelogen": "^0.5.3",
    "eslint": "^8.39.0",
    "husky": "^8.0.3",
    "is-ci": "^3.0.1",
    "kolorist": "^1.8.0",
    "lint-staged": "^13.2.2",
    "minimist": "^1.2.8",
    "nuxt": "^3.4.3",
    "prompts": "^2.4.2",
    "semver": "^7.5.0",
    "tsx": "^3.12.7",
    "typescript": "4.9.5",
    "vexip-ui": "^2.1.18",
    "vitest": "^0.30.1"
  },
  "peerDependencies": {
    "@vexip-ui/icons": ">=1.0.0",
    "vexip-ui": ">=2.1.0"
  }
}
